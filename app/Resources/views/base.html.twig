<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        <base href="" />
        {% block stylesheets %} 
            <link rel="stylesheet" type="text/css" href="{{ asset('bootstrap/css/bootstrap.css') }}" />
            <link rel="stylesheet" type="text/css" href="{{ asset('assets/css/foodrescue.css') }}" />
            <link rel="stylesheet" type="text/css" href="{{ asset('jquery-ui-1.10.4.custom/css/ui-lightness/jquery-ui-1.10.4.custom.min.css') }}" />
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

        
        
    </head>
    <body>
        <div class="header" style="background-color:#8CD968;">
            <div class="container">
                {% block headblock %}
                <div class="row">
                    <div class="col-lg-2 col-md-2 col-sm-8"><a href="/" class="logo">Food Rescue</a></div>
                    {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                        <div class="col-lg-10 col-md-10 col-sm-2">
                            <ul class="navmenu user-settings nav nav-pills pull-right">
                                <li class="username"><a href="#"></a></li>
                                <li class="settings"><a href="{{ path('fos_user_profile_edit') }}">Nustatymai</a></li>
                                <li class="logout"><a href="{{ path('fos_user_security_logout') }}">Atsijungti</a></li>
                            </ul>
                            <ul class="navmenu nav nav-pills pull-right">
                               <!-- <li><a href="{{ path('frame_workers_tm_food_rescue_food_app_dashboard') }}">Pagrindinis </a></li>-->
                                <li><a href="{{ path('frame_workers_tm_food_rescue_food_app_my_products') }}">Produktai </a></li>
                                <li><a href="{{ path('frame_workers_tm_food_rescue_food_app_recipes') }}">Receptai</a></li>
                            </ul>
                        </div>
                    {% else %}
                        <div class="col-lg-10 col-md-10 col-sm-2">
                            <ul class="navmenu nav nav-pills pull-right">
                                <li><a href="{{ path('fos_user_security_login') }}">Prisijungti </a></li>
                                <li><a href="{{ path('fos_user_registration_register') }}">Registruotis</a></li>
                            </ul>
                        </div>
                    {% endif %}

                </div>
                {% endblock %}
            </div>
        </div>
            <div class="container">
                {% block body %}

                {% endblock %}

                {% block javascripts %}
                    <script type="text/javascript" src="{{ asset('jquery-ui-1.10.4.custom/js/jquery-1.10.2.js') }}"></script>
                    <script type="text/javascript" src="{{ asset('jquery-ui-1.10.4.custom/js/jquery-ui-1.10.4.custom.js') }}"></script>
                    <script type="text/javascript" src="{{ asset('assets/js/MyProduct.js') }}"></script>
                    <script src="//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script>
                {% endblock %}
            </div>
    </body>
</html>
